{
    "root": true,
    "overrides": [
      {
        "files": ["*.ts"],
        "extends": ["eslint:recommended", "plugin:@angular-eslint/recommended"],
        "rules": {
          "quotes": ["error", "single"],
          "semi": ["error", "always"],
          "no-console": "warn"
        }
      },
      {
        "files": ["*.html"],
        "extends": ["plugin:@angular-eslint/template/recommended"],
        "rules": {}
      }
    ]
  }
  